<!DOCTYPE html>
<html>
<head>
    <title>Soldier</title>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.8.2/jquery.min.js"></script>
    <script src="/socket.io/socket.io.js"></script>
</head>
<body>

<h1 id="tit">Welcome commander</h1>
Where do you wanna go?: <input type="text" id="url"/><input type="button" id="go" value="GO!"/>



<script>
	


	var socket = io.connect();
    socket.on('connect', function () {
        console.log("me conecte viteh")
    }).on('disconnect', function () {
        $("#go").attr("disabled", "disabled");
        $("#tit").html("Commander is dead");
    });

    $("#go").bind('click',function(){
    	socket.emit("go-here",$("#url").val())
    });

</script>

</body>
</html>